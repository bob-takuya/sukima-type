{
  "name": "typographic-nesting-art-generator",
  "version": "1.2.0",
  "description": "ユーザーがタイピングした文字を、既存の文字の隙間にリアルタイムで最適に配置（回転・拡大縮小）し、画面をタイポグラフィで埋め尽くすインタラクティブなアート作品。",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "type-check": "tsc --noEmit",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:watch": "vitest --watch"
  },
  "dependencies": {
    "opentype.js": "^1.3.4",
    "polygon-clipping": "^0.15.3"
  },
  "devDependencies": {
    "@types/opentype.js": "^1.3.4",
    "typescript": "^5.0.2",
    "vite": "^4.4.5",
    "vitest": "^0.34.6",
    "jsdom": "^22.1.0",
    "@vitest/ui": "^0.34.6",
    "happy-dom": "^12.10.3"
  }
}
